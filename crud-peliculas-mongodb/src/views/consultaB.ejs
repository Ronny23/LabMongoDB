<% include partials/_header %>
<div class="container-fluid">
        <div class="row">
                <!--FORM-->
                <div class="col-md-10 offset-md-1">
                          <table class="table table-bordered table-hover">
                              <thead>
                                <tr>
                                    <th>N°</th>
                                    <th>Nombre</th>
                                    <th>Genero</th>
                                    <th>Fraquicia</th>
                                    <th>Pais</th>
                                    <th>Año</th>
                                    <th>Duración</th>
                                    <th>Compañia</th>
                                    <th>Actores</th>
                                </tr>
                              </thead>
                              <tbody>
                                    <% for(var i=0; i < consultaB.length; i++) { %>
                                      <tr>
                                        <td><%= i + 1%></td>
                                        <td><strong><%= consultaB[i].nombre %></strong></td>
                                        <td><%= consultaB[i].genero %></td>
                                        <td><%= consultaB[i].franquicia %></td>
                                        <td><%= consultaB[i].pais %></td>
                                        <td><%= consultaB[i].anio %></td>
                                        <td><%= consultaB[i].duracion %></td>
                                        <td><%= consultaB[i].compania %></td>
                                        <td><%= consultaB[i].actores %></td>
                                      </tr>
                                    <% } %>
                              </tbody>
                      </table>
                  </div>
              </div>
          </div>
        
</div>
<% include partials/_footer %>